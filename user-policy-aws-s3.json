{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "userPolicyAwsS3",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::NAME_BUCKET",
                "arn:aws:s3:::NAME_BUCKET/*"
            ]
        }
    ]
  }